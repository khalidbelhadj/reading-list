{
  "manifest_version": 3,
  "name": "Bookmarks",
  "version": "1.0.0",
  "description": "Save the current page to your reading list",
  "permissions": ["activeTab"],
  "host_permissions": ["http://localhost:3000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Save bookmark"
  }
}
